<% @progress_reports.each do |p| %>
    <%= link_to p.name, progress_report_path(p) %><br/>
    <%= p.date %><br/>
    <%= p.content %><br/>
    <% if current_user.id == p.user_id %>
        <%= link_to 'Edit This Entry', edit_progress_report_path(p) %>
    <% end %>
    <br/>
    <br/>
<% end %>
<%= link_to 'Add Entry', new_progress_report_path, class:"btn btn-outline-info" %>
